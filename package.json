      {
        "command": "abracadabra.quickFix",
        "key": "alt+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "abracadabra.renameSymbol",
        "key": "f2",
        "when": "editorTextFocus"
      },
      {
        "command": "abracadabra.toggleHighlight",
        "key": "ctrl+alt+h",
        "mac": "ctrl+h",
        "when": "editorTextFocus"
